LoadPlugin python
<Plugin python>
  ModulePath "/opt/signalfx-collectd-plugin"
  LogTraces true
  Interactive false
  Import "signalfx_metadata"
  <Module signalfx_metadata>
    Notifications true
    URL "<%= @signalfx_url_with_dimensions %>"
    Token "<%= @api_token %>"
    NotifyLevel "OKAY"
  </Module>
</Plugin>
