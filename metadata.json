{
  "name": "signalfx-send_collectd_metrics",
  "version": "0.1.0",
  "author": "Uday Sagar Shiramshetty",
  "license": "Apache 2.0",
  "summary": "A puppet module that configures write_http plugin of collectd to send the system metrics to SignalFx",
  "source": "https://github.com/signalfx/puppet_send_metrics",
  "project_page": "https://github.com/signalfx/puppet_send_metrics",
  "issues_url": "https://github.com/signalfx/puppet_send_metrics/issues",
  "dependencies": [
    {"name":"pdxcat/collectd","version_requirement":">= 4.0.0"},
    {"name":"puppetlabs-stdlib","version_requirement":">= 1.0.0"},
    {"name":"puppetlabs/concat","version_requirement":">= 1.0.4"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Amazon",
      "operatingsystemrelease": [
        "2015.03",
        "2014.09"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "15.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.2.0"
    }
  ]
}
